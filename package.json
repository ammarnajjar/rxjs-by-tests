{
  "name": "rxjs-example",
  "version": "0.0.1",
  "description": "Example project to illustrate RxJS features.",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "start": "tsc && node dist/src/index.js",
    "test": "jest --watch",
    "test:ci": "jest",
    "build": "tsc"
  },
  "keywords": [
    "rxjs"
  ],
  "author": "Ammar Najjar <najjarammar@protonmail.com>",
  "license": "MIT",
  "dependencies": {
    "jasmine-marbles": "0.6.0",
    "rxjs": "6.5.5",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "eslint-plugin-jest": "23.0.4",
    "husky": "4.3.8",
    "jest": "29.6.1",
    "prettier": "2.0.4",
    "pretty-quick": "2.0.1",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0"
  }
}
